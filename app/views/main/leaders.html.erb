<div id="leaders">
	<div class="container">
		<div class="row">
			<div class="col-sm-10 col-sm-offset-1">
				<h1>Leaders</h1>
				<h3>Leaders and managers involved in the current incident listed below</h3>

				<% @incident = Incident.find(current_user.incident) %>

				<p>
					<% @incident.managers.split(",").each do |f| %>
						<%= User.find(f).username %> 
					<% end %>
				</p>
			</div>
		</div>
	</div>
</div>