<div id="incidents">
	<div class="container">
		<div class="row">
			<div class='col-sm-8 col-sm-offset-2'>
				<h1>Incidents</h1>
				<%= render 'form' %>

				<% @incidents.each do |f| %>
					<div class="incident">
						<h3><%= f.name %></h3>
						<h4><%= f.location %></h4>
						<p><%= f.managers %></p>
					</div>
				<% end %>
			</div>
		</div>
	</div>
</div>